{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"lib": [
			"es5",
			"es6",
			"dom",
			"es2015.core",
			"es2015.collection",
			"es2015.generator",
			"es2015.iterable",
			"es2015.promise",
			"es2015.proxy",
			"es2015.reflect",
			"es2015.symbol",
			"es2015.symbol.wellknown",
			"esnext.asynciterable"
		],
		"moduleResolution": "node",
		"outDir": "build",
		"declaration": true,
		"strict": true,
		"types": ["node"]
	},
	"include": [
		"index.ts",
		"lib/**/*"
	]
}